-- Show loading screen with progressive loading animation
local function showLoadingScreen()
	local LoadingGui = Instance.new("ScreenGui")
	LoadingGui.Name = "FirengHubLoading"
	LoadingGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	LoadingGui.IgnoreGuiInset = true

	-- Background with image
	local Background = Instance.new("ImageLabel")
	Background.Size = UDim2.new(1, 0, 1, 0)
	Background.Position = UDim2.new(0, 0, 0, 0)
	Background.BackgroundTransparency = 1
	Background.Image = "rbxassetid://6421296789"
	Background.ScaleType = Enum.ScaleType.Crop
	Background.ImageColor3 = Color3.fromRGB(255, 255, 255)
	Background.Parent = LoadingGui

	-- Dark overlay
	local Overlay = Instance.new("Frame")
	Overlay.Size = UDim2.new(1, 0, 1, 0)
	Overlay.Position = UDim2.new(0, 0, 0, 0)
	Overlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Overlay.BackgroundTransparency = 0.4
	Overlay.BorderSizePixel = 0
	Overlay.Parent = LoadingGui

	local Title = Instance.new("TextLabel")
	Title.Size = UDim2.new(0, 300, 0, 40)
	Title.Position = UDim2.new(0.5, -150, 0.4, 0)
	Title.BackgroundTransparency = 1
	Title.Font = Enum.Font.GothamBold
	Title.Text = "Fireng-Hub"
	Title.TextColor3 = Color3.fromRGB(255, 255, 255)
	Title.TextSize = 32
	Title.TextStrokeTransparency = 0.3
	Title.Parent = LoadingGui

	local LoadingText = Instance.new("TextLabel")
	LoadingText.Size = UDim2.new(0, 300, 0, 20)
	LoadingText.Position = UDim2.new(0.5, -150, 0.5, 0)
	LoadingText.BackgroundTransparency = 1
	LoadingText.Font = Enum.Font.Gotham
	LoadingText.Text = "Initializing..."
	LoadingText.TextColor3 = Color3.fromRGB(200, 200, 200)
	LoadingText.TextSize = 14
	LoadingText.TextStrokeTransparency = 0.3
	LoadingText.Parent = LoadingGui

	-- Progress bar background
	local ProgressBg = Instance.new("Frame")
	ProgressBg.Size = UDim2.new(0, 300, 0, 4)
	ProgressBg.Position = UDim2.new(0.5, -150, 0.55, 0)
	ProgressBg.BackgroundColor3 = Color3.fromRGB(40, 40, 45)
	ProgressBg.BorderSizePixel = 0
	ProgressBg.Parent = LoadingGui

	local ProgressBgCorner = Instance.new("UICorner")
	ProgressBgCorner.CornerRadius = UDim.new(1, 0)
	ProgressBgCorner.Parent = ProgressBg

	-- Progress bar fill
	local ProgressFill = Instance.new("Frame")
	ProgressFill.Size = UDim2.new(0, 0, 1, 0)
	ProgressFill.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
	ProgressFill.BorderSizePixel = 0
	ProgressFill.Parent = ProgressBg

	local ProgressFillCorner = Instance.new("UICorner")
	ProgressFillCorner.CornerRadius = UDim.new(1, 0)
	ProgressFillCorner.Parent = ProgressFill

	-- Percentage text
	local PercentText = Instance.new("TextLabel")
	PercentText.Size = UDim2.new(0, 300, 0, 20)
	PercentText.Position = UDim2.new(0.5, -150, 0.58, 0)
	PercentText.BackgroundTransparency = 1
	PercentText.Font = Enum.Font.GothamBold
	PercentText.Text = "0%"
	PercentText.TextColor3 = Color3.fromRGB(88, 101, 242)
	PercentText.TextSize = 12
	PercentText.TextStrokeTransparency = 0.3
	PercentText.Parent = LoadingGui

	LoadingGui.Parent = LP:WaitForChild("PlayerGui")

	-- Loading stages with delays
	local loadingStages = {
		{text = "Loading modules...", delay = 0.3, progress = 15},
		{text = "Initializing ESP system...", delay = 0.4, progress = 30},
		{text = "Setting up World ESP...", delay = 0.3, progress = 45},
		{text = "Loading player features...", delay = 0.3, progress = 60},
		{text = "Configuring visual settings...", delay = 0.3, progress = 75},
		{text = "Loading GUI components...", delay = 0.4, progress = 90},
		{text = "Finalizing setup...", delay = 0.2, progress = 100},
	}

	-- Animate loading stages
	for i, stage in ipairs(loadingStages) do
		LoadingText.Text = stage.text
		TweenService:Create(ProgressFill, TweenInfo.new(stage.delay, Enum.EasingStyle.Quad), {
			Size = UDim2.new(stage.progress / 100, 0, 1, 0)
		}):Play()

		-- Animate percentage
		local startPercent = i == 1 and 0 or loadingStages[i-1].progress
		for p = startPercent, stage.progress do
			PercentText.Text = tostring(p) .. "%"
			task.wait(stage.delay / (stage.progress - startPercent))
		end

		task.wait(0.1)
	end

	-- Success message
	LoadingText.Text = "Loading complete!"
	LoadingText.TextColor3 = Color3.fromRGB(0, 255, 0)
	task.wait(0.3)

	-- Fade out
	TweenService:Create(Background, TweenInfo.new(0.5), {ImageTransparency = 1}):Play()
	TweenService:Create(Overlay, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
	TweenService:Create(Title, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(LoadingText, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(PercentText, TweenInfo.new(0.5), {TextTransparency = 1}):Play()
	TweenService:Create(ProgressBg, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
	TweenService:Create(ProgressFill, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
	task.wait(0.6)
	LoadingGui:Destroy()
end

-- Start loading screen
showLoadingScreen()

-- Utility Functions
local function alive(i)
	if not i then return false end
	local ok = pcall(function() return i.Parent end)
	return ok and i.Parent ~= nil
end
local function validPart(p) return p and alive(p) and p:IsA("BasePart") end
local function clamp(n,lo,hi) if n<lo then return lo elseif n>hi then return hi else return n end end
local function now() return os.clock() end
local function dist(a,b) return (a-b).Magnitude end

local function firstBasePart(inst)
	if not alive(inst) then return nil end
	if inst:IsA("BasePart") then return inst end
	if inst:IsA("Model") then
		if inst.PrimaryPart and inst.PrimaryPart:IsA("BasePart") and alive(inst.PrimaryPart) then return inst.PrimaryPart end
		local p = inst:FindFirstChildWhichIsA("BasePart", true)
		if validPart(p) then return p end
	end
	if inst:IsA("Tool") then
		local h = inst:FindFirstChild("Handle") or inst:FindFirstChildWhichIsA("BasePart")
		if validPart(h) then return h end
	end
	return nil
end

